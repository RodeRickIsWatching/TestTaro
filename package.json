{
  "name": "tarotpi",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite --open --host",
    "build": "tsc && vite build --mode production",
    "build:qa": "vite build --mode test",
    "preview": "vite preview",
    "lint": "yarn eslint && yarn prettier && yarn stylelint",
    "eslint": "eslint --cache '@(src)/**/*.@(js|jsx|ts|tsx)'",
